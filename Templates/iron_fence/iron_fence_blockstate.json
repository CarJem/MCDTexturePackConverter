{
  "multipart": [
    {
      "apply": {
        "model": "minecraft:block/{!}_post_ends"
      }
    },
    {
      "when": {
        "north": "false",
        "west": "false",
        "south": "false",
        "east": "false"
      },
      "apply": {
        "model": "minecraft:block/{!}_post"
      }
    },
    {
      "when": {
        "north": "true",
        "west": "false",
        "south": "false",
        "east": "false"
      },
      "apply": {
        "model": "minecraft:block/{!}_cap"
      }
    },
    {
      "when": {
        "north": "false",
        "west": "false",
        "south": "false",
        "east": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_cap",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false",
        "west": "false",
        "south": "true",
        "east": "false"
      },
      "apply": {
        "model": "minecraft:block/{!}_cap_alt"
      }
    },
    {
      "when": {
        "north": "false",
        "west": "true",
        "south": "false",
        "east": "false"
      },
      "apply": {
        "model": "minecraft:block/{!}_cap_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_side_alt"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_side_alt",
        "y": 90
      }
    }
  ]
}