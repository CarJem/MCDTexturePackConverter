{
  "multipart": [
    {
      "apply": {
        "model": "minecraft:block/{!}_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_side_alt"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "minecraft:block/{!}_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "minecraft:block/{!}_noside"
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "minecraft:block/{!}_noside_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "minecraft:block/{!}_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "minecraft:block/{!}_noside",
        "y": 270
      }
    }
  ]
}